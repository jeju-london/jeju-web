extends layout

block css
  link(rel='stylesheet', href='/stylesheets/content.css')

block tab
  include tab.pug

block content
  div(class="container")
    div(class="starter-template")
      h1 Bootstrap starter template
      p(class="lead") Use this document as a way to quickly start any new project.<br> All you get is this text and a mostly barebones HTML document.
      form(class="form-signin" action="/logout" method="post")
        button(class="btn btn-lg btn-primary btn-block" type="submit") Log Out